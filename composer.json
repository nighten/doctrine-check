{
  "name": "nighten/doctrine-check",
  "type": "library",
  "description": "Provides check for doctrine type mapping",
  "keywords": [
    "debug",
    "doctrine",
    "static analysis",
    "dev",
    "inspection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Golovanov",
      "email": "aa.golovanov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "doctrine/orm": "^2.0|^3.0",
    "phpstan/phpdoc-parser": "^1.0",
    "symfony/console": "^6.4|^7.0"
  },
  "bin": [
    "bin/doctrine-check"
  ],
  "require-dev": {
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-strict-rules": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Nighten\\DoctrineCheck\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nighten\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev"
}
