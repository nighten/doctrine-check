includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
    level: 9
    paths:
        - Check/
        - Config/
        - Console/
        - Doctrine/
        - Dto/
        - Exception/
        - Ignore/
        - Php/
        - Type/
    excludePaths:
        analyseAndScan:
            # it depends on PhpdocParser version
            - Php/PHPDocParser/Bridge/PhpstanPhpdocParserV*.php
